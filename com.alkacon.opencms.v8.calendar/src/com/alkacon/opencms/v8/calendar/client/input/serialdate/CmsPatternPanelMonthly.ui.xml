<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui.input"
	xmlns:cmsd="urn:import:org.opencms.gwt.client.ui.input.datebox">
	<ui:with type="org.opencms.acacia.client.css.I_CmsWidgetsLayoutBundle" field="widgets" />
	<ui:with type="com.alkacon.opencms.v8.calendar.client.widget.css.I_CmsLayoutBundle" field="css" />
	<g:HTMLPanel styleName="">
		<div class="{css.widgetCss.patternRow}">
			<cms:CmsRadioButton ui:field="m_dayMonthRadioButton" addStyleNames="{css.widgetCss.patternRadio}" /><div class="{widgets.widgetCss.textBox}"><g:TextBox ui:field="m_everyDay" styleName="{css.widgetCss.patternInput}" /></div><span ui:field="m_labelDays"></span><div class="{widgets.widgetCss.textBox}"><g:TextBox ui:field="m_everyMonth" styleName="{css.widgetCss.patternInput}" /></div><span ui:field="m_labelMonths"></span>
		</div>
		<div class="{css.widgetCss.patternRow}">
			<cms:CmsRadioButton ui:field="m_weekDayMonthRadioButton" addStyleNames="{css.widgetCss.patternRadio}" /><cms:CmsSelectBox ui:field="m_atWeek" /><cms:CmsSelectBox ui:field="m_atDay" /><span ui:field="m_labelEvery"></span><div class="{widgets.widgetCss.textBox}"><g:TextBox ui:field="m_atMonth" styleName="{css.widgetCss.patternInput}" /></div><span ui:field="m_labelMonth"></span>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>